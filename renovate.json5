{
    '$schema': 'https://docs.renovatebot.com/renovate-schema.json',
    extends: [
        'github>Lombiq/renovate-config:default.json5',
        'github>Lombiq/Open-Source-Orchard-Core-Extensions:renovate-osoce-submodule.json5',
        // Due to having to update branch references, it's easier to test all dependencies at once.
        'github>Lombiq/renovate-config:all-dependencies.json5',
    ],
    packageRules: [
        {
            matchPackageNames: ['azure/webapps-deploy'],
            matchFileNames: ['.github/workflows/deploy-orchard1-to-azure-app-service.yml'], 
            enabled: false,
        },
        // Disabling until https://github.com/NishargShah/eslint-config-airbnb-extended/issues/8 is fixed.
        {
            matchPackageNames: ['@stylistic/eslint-plugin', '@eslint/compat'],
            enabled: false,
        },
    ]
}
